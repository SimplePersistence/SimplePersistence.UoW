{
  "version": "3.3.0",
  "title": "SimplePersistence.UoW",
  "description": "SimplePersistence.UoW is a framework to help implement the Unit of Work pattern, by exposing interfaces for repositories that can be aggregated in work areas.",
  "authors": [ "João Simões" ],
  "copyright": "Copyright © 2016 SimplePersistence",
  "packOptions": {
    "summary": "Framework to help implement the Unit of Work pattern.",
    "tags": [ "unit of work", "uow", "persistence", "pattern" ],
    "projectUrl": "https://github.com/SimplePersistence/SimplePersistence.UoW",
    "licenseUrl": "https://github.com/SimplePersistence/SimplePersistence.UoW/blob/master/LICENSE",
    "iconUrl": "https://avatars3.githubusercontent.com/u/17026510",
    "releaseNotes": "All repositories now extend from a common interface named ISimplePersistenceRepository\nAdded support for .NET Standard",
    "repository": {
      "type": "git",
      "url": "https://github.com/SimplePersistence/SimplePersistence.UoW"
    }
  },
  "buildOptions": {
    "xmlDoc": true,
    "optimize": true,
    "keyFile": "SimplePersistence.snk"
  },

  "frameworks": {
    ".NETPortable,Version=v4.0,Profile=Profile328": {
      "buildOptions": {
        "define": [ "PORTABLE40" ]
      },
      "frameworkAssemblies": {
        "mscorlib": "",
        "System": "",
        "System.Core": ""
      }
    },
    "net20": {},
    "net35": {},
    "net40": {},
    "net45": {},
    "netstandard1.0": {
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
    }
  }
}
